<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EsriMapNavigation.MainPage">

    <StackLayout>
        <Button Text="Click to open Esri ArcGIS map"
                VerticalOptions="CenterAndExpand"
                HorizontalOptions="Center"
                Clicked="OnEsriButtonClicked" />
        <Button Text="Click to open Xamarin.Forms map"
                VerticalOptions="CenterAndExpand"
                HorizontalOptions="Center"
                Clicked="OnFormsButtonClicked" />
    </StackLayout>

</ContentPage>
